{
  "coc.preferences.formatOnSaveFiletypes": [
    "js",
    "javascript",
    "javascriptreact",
    "jsx",
    "ts",
    "tsx",
    "typescript",
    "typescriptreact",
    "json",
    "css"
  ],
  "colors.filetypes": ["*"],
  "dialog.floatBorderHighlight": "CocFloatingBorder",
  "dialog.floatHighlight": "CocFloating",
  "suggest.floatConfig": {
    "border": true,
    "winblend": 15,
    "shadow": true,
    "borderhighlight": "FloatBorder"
  },
  "hover.floatConfig": {
    "border": true,
    "winblend": 15,
    "rounded": true,
    "shadow": true,
    "borderhighlight": "FloatBorder"
  },
  "signature.floatConfig": {
    "border": true,
    "winblend": 15,
    "rounded": true,
    "shadow": true,
    "borderhighlight": "FloatBorder"
  },
  "diagnostic.floatConfig": {
    "border": true,
    "winblend": 15,
    "rounded": true,
    "shadow": true,
    "borderhighlight": "FloatBorder"
  },
  "snippets.userSnippetsDirectory": "$HOME/.dotfiles/nvim/snippets/ultisnips"
}
